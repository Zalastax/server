<div class="ui grid">
  <%= render("sidebar.html", items: main_sidebar_items(@conn, @assignment), active_item: "Submit") %>
  <div class="thirteen wide column">
    <div class="ui segment">
    <%= form_for @conn, Routes.submission_path(@conn, :submit, @assignment.id), [multipart: true], fn f -> %>
      <%= file_input f, :file %>
      <%= submit "Upload" %>
    <% end %>
    </div>
  </div>
</div>